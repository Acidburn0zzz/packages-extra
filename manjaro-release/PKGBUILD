pkgname=manjaro-release
pkgver=0.8.3
pkgrel=1
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('b60daecda58fc6368660a4cf5a3744b5e2a64882a1da45c68d65edd25280417a')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}

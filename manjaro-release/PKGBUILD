pkgname=manjaro-release
pkgver=0.2
pkgrel=4
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('dbfa3cc061eac0fa0df2efa5a199394d03520c4226e7a9f74b8ca5f6daca2c29')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}

pkgname=manjaro-release
pkgver=0.2
pkgrel=3
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('6029b5397b193ac1a22a7c4c7152714b7461b4d24be526770f0d48331c3929ce')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}
